{
  "fieldWeights": {
    "title": 3.0,
    "desc": 1.6,
    "url": 0.6
  },
  "baseline": 0,
  "minMargin": 1.0,
  "categories": [
    {
      "label": "Health & Medicine",
      "include": [
        "doctor", "dr\\.?\\s", "md\\b", "physician", "medicine", "medical", "cardio", "metabolic", "endocrin",
        "neuro", "sleep", "nutrition", "diet", "fasting", "cholesterol", "insulin", "glucose", "exercise physiology"
      ],
      "exclude": [
        "doctor mix", "dr\\.?\\s?dre", "doctor who", "drum", "dj\\b"
      ],
      "threshold": 1.2
    },
    {
      "label": "AI & Emerging Tech",
      "include": [
        "ai", "artificial intelligence", "gpt", "chatgpt", "llm", "deep learning",
        "machine learning", "ml\\b", "generative", "midjourney", "stable diffusion", "prompt",
        "karpathy", "openai", "anthropic", "llama", "mistral"
      ],
      "exclude": [
        "makeup ai", "ai art nails"
      ],
      "threshold": 1.1
    },
    {
      "label": "General Tech & Reviews",
      "include": [
        "tech", "review", "unboxing", "gadgets?", "apple", "iphone", "ipad", "mac",
        "android", "windows", "pc build", "benchmark",
        { "re": "(?i)\\b(review(s)?|unboxing|tech|gadget(s)?)\\b", "w": 1.0 },
        { "re": "(?i)\\b(apple|android|iphone|mac|windows)\\b", "w": 0.7 }
      ],
      "exclude": [
        "photography", "camera", "drone", "flight sim", "tutorial", "tutorials",
        { "re": "(?i)\\b(camera|photography|filmmaking)\\b" }
      ],
      "threshold": 1.3
    },
    {
      "label": "Photography & Cameras",
      "include": [
        "camera", "photograph", "lens", "sony", "canon", "nikon", "fujifilm",
        "lightroom", "photoshop", "mirrorless", "brandon li", "allan walls",
        { "re": "(?i)\\b(camera|photograph(y|er)|filmmak(e|ing)|lens(es)?|gimbal)\\b", "w": 1.0 },
        { "re": "(?i)\\b(sony|canon|nikon|fujifilm|lumix|insta360|gopro|blackmagic)\\b", "w": 0.9 }
      ],
      "exclude": [
        "security camera", "body cam", "dashcam", "marketing", "dropship", "ecom", "crypto", "stocks"
      ],
      "threshold": 1.3
    },
    {
      "label": "Video Editing & Creative Tools",
      "include": [
        "edit", "premiere", "final cut", "davinci resolve", "after effects",
        "motionvfx", "filmora", "bretfx", "color grade", "timeline"
      ],
      "exclude": [
        "sales", "agency", "dropship"
      ],
      "threshold": 1.2
    },
    {
      "label": "Business & Marketing",
      "include": [
        "marketing", "ads", "adwords", "funnels", "funnel", "sales", "saas",
        "ecommerce", "brand", "branding", "agency", "entrepreneur", "entrepreneurship",
        "startup", "shopify", "amazon fba",
        { "re": "(?i)\\b(startup(s)?|entrepreneur(ship)?|SaaS|agency|e[- ]?commerce|marketing|sales|funnel|brand(ing)?)\\b", "w": 1.0 }
      ],
      "exclude": [
        "flight", "storm", "camera", "lens", "photo", "premiere", "filmora",
        "davinci", "final cut", "garden", "seed", "compost", "orchestra",
        "guitar", "mix", "piano", "trailer", "trailers", "clip", "clips",
        "cinema", "movie", "film", "entertainment",
        { "re": "(?i)\\bmusic marketing\\b" }
      ],
      "threshold": 1.2
    },
    {
      "label": "Music & Musicians",
      "include": [
        "guitar", "bass", "drums", "piano", "vocal", "singer", "songwriter",
        "mix", "master", "daw", "ableton", "logic pro", "pro tools", "pedal",
        "riff", "chord", "jazz",
        { "re": "(?i)\\b(official)\\b.*\\b(album|single|records|band|tour|live set)\\b", "w": 1.2 },
        { "re": "(?i)\\b(lyrics?|guitar(ist)?|drums?|DJ|beats?|remix)\\b", "w": 0.8 }
      ],
      "exclude": [
        "storm", "flight", "pilot", "camera", "lens", "tutorial", "tutorials", "review", "reviews",
        { "re": "(?i)\\btutorial(s)?\\b" },
        { "re": "(?i)\\breview(s)?\\b" }
      ],
      "threshold": 1.2
    },
    {
      "label": "DIY, Home & Construction",
      "include": [
        "diy", "home", "renovation", "woodworking", "craftsman", "garage", "concrete", "builder",
        { "re": "(?i)\\bDIY\\b|\\bhow[- ]to\\b|\\b(build|builder|building|repair|fix(ing)?)\\b|\\btutorial\\b|\\bwoodworking\\b", "w": 1.0 }
      ],
      "exclude": [
        { "re": "(?i)\\bDIY (music|beats)\\b" }
      ],
      "threshold": 1.0
    },
    {
      "label": "News",
      "include": [
        { "re": "(?i)\\b(news|newscast|headlines|breaking news|world news)\\b", "w": 1.0 },
        { "re": "(?i)\\b(news (update|report|analysis|digest)|briefing)\\b", "w": 0.9 }
      ],
      "exclude": [
        "product news", "game news", "games news", "review", "reviews",
        { "re": "(?i)\\b(product|game|camera)\\s+news\\b" },
        { "re": "(?i)\\breview(s)?\\b" }
      ],
      "threshold": 1.8
    },
    {
      "label": "Gardening",
      "include": [
        { "re": "(?i)\\b(garden(ing)?|gardener|raised beds?|compost|permaculture|homestead(ing)?)\\b", "w": 1.0 },
        { "re": "(?i)\\b(lawn|landscap(e|ing))\\b", "w": 0.8 }
      ],
      "exclude": [
        { "re": "(?i)\\bgarden (route|state)\\b" }
      ],
      "threshold": 2.0
    },
    {
      "label": "Aviation",
      "include": [
        { "re": "(?i)\\b(aviation|pilot(s)?|airline(s)?|airbus|boeing|ATC)\\b", "w": 1.0 },
        { "re": "(?i)\\b(flight (sim|simulator|training)|MSFS|X-Plane|VATSIM)\\b", "w": 0.9 }
      ],
      "exclude": [
        { "re": "(?i)\\b(drone photography)\\b" }
      ],
      "threshold": 2.0
    }
  ],
  "labelAliases": {
    "AI / ML": "AI & Emerging Tech",
    "Tech Reviews": "General Tech & Reviews"
  }
}
